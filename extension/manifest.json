{
    "name": "Focus",
    "author": "Brad Jasper",
    "version": "2.8",
    "manifest_version": 2,
    "description": "Official browser extension for Focus, a macOS app that blocks distracting websites and applications so you can be more productive.",
    "icons": {
        "16": "icon_16.png",
        "48": "icon_48.png",
        "128": "icon_128.png"
    },
    "permissions": ["webNavigation", "tabs"],
    "background": {
        "scripts": [
            "browserdetect.js",
            "config.js",
            "reconnecting-websocket.js",
            "focus-connection.js",
            "focus.js"
        ]
    }
}
